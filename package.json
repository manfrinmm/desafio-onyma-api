    "@swc/jest": "^0.2.20",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
